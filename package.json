{
  "name": "react-template",
  "version": "1.0.0",
  "description": "react template with tailwindcss",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch:css && webpack serve --mode development --open --hot",
    "build": "npm run build:css && webpack --mode production",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "watch:css": "postcss src/tailwind.css -o src/index.css -w"
  },
  "author": "elias velardez",
  "license": "MIT",
  "devDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "dependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "html-webpack-plugin": "^5.3.1",
    "postcss": "^8.3.1",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.0.0",
    "postcss-nesting": "^8.0.1",
    "postcss-preset-env": "^5.3.0",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.1.4",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "postcss-cli": "^8.3.1"
  }
}
